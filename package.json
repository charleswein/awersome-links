{
  "name": "awesome-links",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "pre-commit": "lint-staged",
    "post-commit": "git update-index --again"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "graphql": "^16.5.0",
    "next": "12.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.16.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.1",
    "@types/jest": "^28.1.6",
    "@types/node": "18.6.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "babel-jest": "^28.1.3",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.3",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "stylelint": "^14.9.1",
    "ts-jest": "^28.0.7",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.css": [
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint pages/ --ext .ts,.tsx --fix"
    ]
